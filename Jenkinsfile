pipeline {
    agent any
    stages {
        stage('fetching') {
            steps {
                sh ' echo fetching '
            }
        
        }
        stage('building') { 
            steps {
                echo 'building'
            }
        }
        stage('testing') {
            steps {
                echo 'testing'
            }
        }
        stage('deploying') {
            steps {
                echo ' deploying '
            }
        }
    }
    
}